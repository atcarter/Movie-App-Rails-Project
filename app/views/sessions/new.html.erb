<%= form_for(:user, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %> 
  <br><br>
  <%= f.submit "Log In!" %>
<% end %>

<br>
Don't Have an account? <%= link_to "Sign up here!", signup_path %> or <%= link_to "Sign in with Google!", '/auth/google_oauth2', method: :post %>

